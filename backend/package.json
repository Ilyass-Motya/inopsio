{
  "name": "inopsio-backend",
  "version": "1.0.0",
  "description": "Inopsio Backend - Multi-tenant SaaS platform backend services",
  "private": true,
  "workspaces": [
    "auth-service",
    "user-service",
    "ai-service",
    "crm-service",
    "erp-service",
    "email-service",
    "workflow-service",
    "inosec-core-service",
    "inosec-edge-service",
    "monitoring-service",
    "gateway"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev --workspaces\"",
    "build": "npm run build --workspaces",
    "start": "npm run start --workspaces",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist coverage",
    "docker:build": "docker build -t inopsio-backend .",
    "docker:run": "docker run -p 3000:3000 inopsio-backend",
    "db:migrate": "npm run db:migrate --workspaces",
    "db:seed": "npm run db:seed --workspaces",
    "db:reset": "npm run db:reset --workspaces"
  },
  "dependencies": {
    "express": "^4.18.0",
    "cors": "^2.8.0",
    "helmet": "^7.1.0",
    "compression": "^1.7.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.0",
    "bcryptjs": "^2.4.0",
    "jsonwebtoken": "^9.0.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "mongoose": "^8.0.0",
    "sequelize": "^6.35.0",
    "pg": "^8.11.0",
    "redis": "^4.6.0",
    "ioredis": "^5.3.0",
    "axios": "^1.6.0",
    "node-cron": "^3.0.0",
    "nodemailer": "^6.9.0",
    "socket.io": "^4.7.0",
    "ws": "^8.14.0",
    "multer": "^1.4.0",
    "sharp": "^0.32.0",
    "uuid": "^9.0.0",
    "lodash": "^4.17.0",
    "moment": "^2.29.0",
    "date-fns": "^2.30.0",
    "joi": "^17.11.0",
    "yup": "^1.3.0",
    "express-rate-limit": "^7.1.0",
    "express-validator": "^7.0.0",
    "swagger-jsdoc": "^6.2.0",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.11.0",
    "pino": "^8.16.0",
    "pino-pretty": "^10.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/express": "^4.17.0",
    "@types/cors": "^2.8.0",
    "@types/compression": "^1.7.0",
    "@types/morgan": "^1.9.0",
    "@types/bcryptjs": "^2.4.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.0",
    "@types/passport-local": "^1.0.0",
    "@types/multer": "^1.4.0",
    "@types/uuid": "^9.0.0",
    "@types/lodash": "^4.14.0",
    "@types/jest": "^29.5.0",
    "typescript": "^5.2.0",
    "ts-node": "^10.9.0",
    "nodemon": "^3.0.0",
    "concurrently": "^8.2.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.0",
    "eslint": "^8.52.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/inopsio/inopsio.git",
    "directory": "backend"
  },
  "keywords": [
    "nodejs",
    "express",
    "typescript",
    "microservices",
    "saas",
    "backend"
  ],
  "author": "Inopsio Team",
  "license": "MIT"
}
